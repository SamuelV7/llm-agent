{
    "service": "llm-agent",
    "workspaceFolder": "/workspace",
    "dockerComposeFile": "docker-compose.yaml",
    "customizations": {
        "vscode": {
            "extensions": [
                "golang.go",
                "ms-python.python",
                "ms-python.vscode-pylance"

            ]
        },
        "settings": {
